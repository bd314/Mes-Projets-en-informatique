
public class sdzl {

	public static void main(String[] args) {
		
		System.out.println("Hello world !\nI'm the \'best\'");
		
		//test des variables et des calculs
		int nb , nb2 ;
		String text;
		text = "Ceci est un texte stoké dans une variable";
		
		nb = 10 ;
		nb2 = 20 ;
		int result = nb2 - nb ;
		
		System.out.println(text);
		System.out.println(text);
		
		System.out.println("Le résultat est = " + result);

	}

}
